                            if ((val.cargo_empleado == 'JEFE LABORATORIO') && (val.sede == 'LINCE')) {
                                //alert(val.nombre_empleado);
                                $('.con-json select').append('<option value="' + val.id_empleado + '">' + val.nombre_empleado + '</option>');
                            }
                        });

                    },
